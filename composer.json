{
    "name": "commerceguys/guzzle-oauth2-plugin",
    "description": "An OAuth2 plugin (subscriber) for Guzzle",
    "license": "MIT",
    "require": {
        "guzzlehttp/guzzle": "~5.0",
        "firebase/php-jwt": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "CommerceGuys\\Guzzle\\Oauth2\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "doctrine/cache": "~1.3"
    },
    "suggest": {
        "doctrine/cache": "Allow token persistence in doctrine cache"
    },
    "autoload-dev": {
        "psr-4": {
            "CommerceGuys\\Guzzle\\Oauth2\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Bojan Zivanovic"
        },
        {
            "name": "Damien Tournoud"
        },
        {
            "name": "Patrick Dawkins"
        },
        {
            "name": "Xavier Lembo"
        }
    ],
    "config": {
        "bin-dir": "bin"
    }
}
